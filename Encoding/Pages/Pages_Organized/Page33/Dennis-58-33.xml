    <text>     
        <body>
            

            
            <div type="page" xml:id="pg_33">   
                
                <div type="panelGrp" xml:id="eg_0001">
                    
                    
                    
                    <cbml:panel n="1" characters="#dennis #tommy #wilson">
                        <cbml:balloon type="speech" who="#wilson">   
                            <emph rendition="#jaggies #color:red">HALP!</emph>            
                        </cbml:balloon>  
                        <cbml:balloon type="speech" who="#dennis">
                            Look at Mr. Wilson <emph rendition="#b">GO!</emph>
                        </cbml:balloon>
                        <cbml:balloon type="speech" who="#tommy">
                            <emph rendition="#color:red">YIPPEE!</emph>
                        </cbml:balloon>
                        <move who="#wilson" rendition="#fast"/>
                        <note type="panelDesc"> Dennis and Tommy watch as Mr. Wilson goes very quickly down a hill surrounded by trees on a pair of snowshoes
                        </note> 
                    </cbml:panel>
                    
                    
                    <cbml:panel n="2" characters="#wilson">
                        <move who="#wilson" rendition="#fast"/>
                        <move who="#wilson" rendition="#excite"/>
                        <note type="panelDesc"> Mr. Wilson looks scared as he continues accidentally skiing down the hill
                        </note> 
                    </cbml:panel>
                    
                    <cbml:panel n="3" characters="#wilson">
                        <move who="#wilson" rendition="#fast"/>
                        <move who="#wilson" rendition="#excite"/>
                        <note type="panelDesc"> Mr. Wilson covers his eyes as he comically swerves around several trees without hitting them
                        </note> 
                    </cbml:panel>
                    
                    <cbml:panel n="4" characters="#wilson">
                        <cbml:balloon type="speech" who="#wilson">   
                            <emph rendition="#jaggies #color:red">MARTHA!</emph>            
                        </cbml:balloon> 
                        <move who="#wilson" rendition="#fast"/>
                        <move who="#wilson" rendition="#excite"/>
                        <note type="panelDesc"> Mr. Wilson spreads his arms and feet as he launches off a hill
                        </note> 
                    </cbml:panel>
                    
                    <cbml:panel n="5" characters="#wilson"> 
                        <move who="#wilson" rendition="#fast"/>
                        <move who="#wilson" rendition="#excite"/>
                        <note type="panelDesc"> Mr. Wilson falls on his butt into the snow with a large puff of snow
                        </note> 
                    </cbml:panel>
                    
                    
                   
                    
                    
                </div>
                <pb/>    
            </div>
        </body>
